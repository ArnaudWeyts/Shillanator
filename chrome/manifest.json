{
  "manifest_version": 2,

  "name": "__MSG_extName__",
  "description": "__MSG_extDescription__",
  "version": "0.1",
  "default_locale": "en",
  "permissions": [
      "<all_urls>"
  ],
  "icons": {
    "16": "./img/icon16.png",
    "48": "./img/icon48.png",
    "128": "./img/icon128.png"
  },
  "page_action": {
    "default_icon": "./img/icon19.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["./js/shillanator.js"]
    }
  ]
}